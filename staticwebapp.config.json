{
  "routes": [
    {
      "route": "/*",
      "headers": {
        "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https: data: blob:"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "*.js", "*.css", "*.json", "*.png", "*.jpg", "*.svg"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".css": "text/css",
    ".svg": "image/svg+xml",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".ico": "image/x-icon"
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}
