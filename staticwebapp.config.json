{
  "routes": [
    {
      "route": "/assets/*.js",
      "headers": {
        "content-type": "text/javascript; charset=utf-8"
      }
    },
    {
      "route": "/assets/*.mjs",
      "headers": {
        "content-type": "text/javascript; charset=utf-8"
      }
    },
    {
      "route": "/assets/*.css",
      "headers": {
        "content-type": "text/css; charset=utf-8"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "content-type": "text/javascript; charset=utf-8"
      }
    },
    {
      "route": "/*",
      "headers": {
        "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https: data: blob:"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "*.js", "*.mjs", "*.css", "*.json", "*.png", "*.jpg", "*.svg", "*.ico"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".css": "text/css",
    ".svg": "image/svg+xml",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".ico": "image/x-icon",
    ".wasm": "application/wasm"
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff"
  }
}
